<style>
#header {display:none;}
#footer {display:none;}
</style>


<div class = "span3 offset3" align="center">
  <h1 class="sign_up_header">Ruby Benefit Fund</h1> 
</div>

<div class="row">
<div class="offset3 span6">
<div class="border-form-div">

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :username %><br />
  <%= f.text_field :username %></div>

 <div><%= f.label :mobile_number %><br />
  <%= f.text_field :mobile_number %></div>

  <div><%= f.label :Address %><br/>
  <%= f.text_area :Address, :id => "Address", :cols => 22, :rows => 3 %></div>
      
  <div><%= f.label :email %><br />
  <%= f.email_field :email, :autofocus => true %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.label :role %><br />
  <%= f.select :role, options_for_select([["Manager", "manager"], ["Deo",  "deo"]]), { :include_blank => true } %>
 </div><br /><br />

  <div><%= f.submit "Sign up" ,class: "btn btn-primary" %></div>
  <% end %>
  </div>
 </div>
</div>

